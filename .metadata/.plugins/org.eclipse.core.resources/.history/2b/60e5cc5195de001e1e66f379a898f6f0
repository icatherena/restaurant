package com.restaurant.recipes.model;

import java.util.List;

public class Ingredient {
	
	private Long id;
	private String name;
	private List<Recipe> itemRecipe;
	
	public Ingredient() {
		super();
	}

	public Ingredient(Long id, String name, List<Recipe> itemRecipe) {
		super();
		this.id = id;
		this.name = name;
		this.itemRecipe = itemRecipe;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public List<Recipe> getItemRecipe() {
		return itemRecipe;
	}

	public void setItemRecipe(List<Recipe> itemRecipe) {
		this.itemRecipe = itemRecipe;
	}

	@Override
	public String toString() {
		return "Ingredient [id=" + id + ", name=" + name + ", itemRecipe=" + itemRecipe + "]";
	}
	
}
